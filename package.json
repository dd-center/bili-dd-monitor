{
  "name": "bili-dd-monitor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": " ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-start": "tsc -p ./electron/ && electron -p ./dist/electron/src/main.js"
  },
  "main": "./dist/electron/src/main.js",
  "private": true,
  "dependencies": {
    "electron-settings": "^3.2.0",
    "ngx-electron": "^2.1.1",
    "request": "^2.88.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "ng-zorro-antd": "^8.3.1",
    "tslib": "^1.10.0",
    "rxjs": "~6.4.0",
    "zone.js": "~0.9.1",
    "@types/electron-settings": "^3.1.1",
    "@types/request": "^2.48.3",
    "@types/socket.io-client": "^1.4.32",
    "@angular/animations": "~8.2.9",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~8.2.9",
    "@angular/forms": "~8.2.9",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "electron": "^6.0.11",
    "@angular-devkit/build-angular": "~0.803.8",
    "@angular/cli": "~8.3.8",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "build": {
    "productName": "BILI-DD-MONITOR",
    "appId": "com.kinori.bili_dd_monitor",
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "./public/icon256.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../public/icon256.ico",
      "uninstallerIcon": "./public/icon256.ico",
      "installerHeaderIcon": "./public/icon256.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "DD监控室"
    }
  }
}